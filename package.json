    "json5": "2.2.3",
    "jspdf": "3.0.4",
    "kolorist": "1.8.0",
    "naive-ui": "2.43.2",
    "pagedjs": "0.4.3",
    "pinia": "3.0.4",
    "signature_pad": "5.1.3",
    "swiper": "12.0.3",
    "tailwind-merge": "3.4.0",
    "typeit": "8.8.7",
    "vue": "3.5.26",
    "vue-draggable-plus": "0.6.0",
    "vue-i18n": "11.2.7",
    "vue-json-pretty": "2.6.0",
    "vue-pdf-embed": "2.1.3",
    "vue-router": "4.6.4",
    "vue3-google-map": "0.26.0",
    "wavesurfer.js": "7.12.1",
    "xgplayer": "3.0.23",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.7.3",
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@iconify/json": "2.2.422",
    "@sa/scripts": "workspace:*",
    "@types/d3": "7.4.3",
    "@types/dagre": "0.7.53",
