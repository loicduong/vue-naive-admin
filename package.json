{
  "name": "vue-naive-admin",
  "type": "module",
  "version": "0.0.2",
  "description": "A fresh and elegant admin template, based on Vue3、Vite3、TypeScript、NaiveUI and UnoCSS.",
  "author": {
    "name": "Loic Duong",
    "email": "dploc96@gmail.com",
    "url": "https://github.com/loicduong"
  },
  "license": "MIT",
  "homepage": "https://github.com/loicduong/vue-naive-admin",
  "repository": {
    "url": "https://github.com/loicduong/vue-naive-admin.git"
  },
  "bugs": {
    "url": "https://github.com/loicduong/vue-naive-admin/issues"
  },
  "keywords": [
    "vue 3",
    "vite 5",
    "typescript",
    "naive-ui",
    "unocss"
  ],
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:tst": "vite build --mode testing",
    "cleanup": "sa cleanup",
    "commit": "commitlint --edit",
    "dev": "vite",
    "dev:prd": "vite --mode production",
    "dev:tst": "vite --mode testing",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@antv/data-set": "0.11.8",
    "@antv/g2": "5.2.7",
    "@antv/g6": "5.0.30",
    "@better-scroll/core": "2.5.1",
    "@iconify/vue": "4.1.2",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "@visactor/vchart": "1.12.11",
    "@visactor/vchart-theme": "1.12.2",
    "@vueuse/components": "11.2.0",
    "@vueuse/core": "11.1.0",
    "clipboard": "2.0.11",
    "dayjs": "1.11.13",
    "echarts": "5.5.1",
    "jsbarcode": "3.11.6",
    "json5": "2.2.3",
    "naive-ui": "2.40.1",
    "nprogress": "0.2.0",
    "pinia": "2.2.4",
    "print-js": "1.6.0",
    "swiper": "^11.1.14",
    "tailwind-merge": "2.5.4",
    "typeit": "^8.8.7",
    "vditor": "3.10.7",
    "vue": "3.5.12",
    "vue-draggable-plus": "0.5.4",
    "vue-i18n": "10.0.4",
    "vue-pdf-embed": "2.1.1",
    "vue-router": "4.4.5",
    "xgplayer": "3.0.20"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.8.0",
    "@commitlint/cli": "19.5.0",
    "@commitlint/config-conventional": "19.5.0",
    "@iconify/json": "2.2.264",
    "@sa/scripts": "workspace:*",
    "@types/html-minifier-terser": "7.0.2",
    "@types/node": "22.8.1",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "0.63.6",
    "@unocss/preset-icons": "0.63.6",
    "@unocss/preset-uno": "0.63.6",
    "@unocss/transformer-directives": "0.63.6",
    "@unocss/transformer-variant-group": "0.63.6",
    "@unocss/vite": "0.63.6",
    "@vitejs/plugin-vue": "5.1.4",
    "@vitejs/plugin-vue-jsx": "4.0.1",
    "eslint": "9.13.0",
    "eslint-plugin-format": "0.1.2",
    "html-minifier-terser": "7.2.0",
    "lint-staged": "15.2.10",
    "sass": "1.80.4",
    "simple-git-hooks": "2.11.1",
    "tsx": "4.19.2",
    "typescript": "5.6.3",
    "unplugin-auto-import": "0.18.3",
    "unplugin-icons": "0.19.3",
    "unplugin-vue-components": "0.27.4",
    "unplugin-vue-router": "0.10.8",
    "vite": "5.4.10",
    "vite-plugin-progress": "0.0.7",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "7.5.4",
    "vite-plugin-vue-layouts": "0.11.0",
    "vue-tsc": "2.1.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9.13.0"
      }
    }
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm commit",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "volta": {
    "node": "22.11.0",
    "pnpm": "9.12.3"
  }
}
