{
  "name": "vue-naive-admin",
  "type": "module",
  "version": "0.0.3",
  "description": "A fresh and elegant admin template, based on Vue3、Vite3、TypeScript、NaiveUI and UnoCSS.",
  "author": {
    "name": "Loic Duong",
    "email": "dploc96@gmail.com",
    "url": "https://github.com/loicduong"
  },
  "license": "MIT",
  "homepage": "https://github.com/loicduong/vue-naive-admin",
  "repository": {
    "url": "https://github.com/loicduong/vue-naive-admin.git"
  },
  "bugs": {
    "url": "https://github.com/loicduong/vue-naive-admin/issues"
  },
  "keywords": [
    "vue 3",
    "vite 5",
    "typescript",
    "naive-ui",
    "unocss"
  ],
  "engines": {
    "node": ">=18.12.0",
    "pnpm": ">=8.7.0"
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:tst": "vite build --mode testing",
    "cleanup": "sa cleanup",
    "commit": "commitlint --edit",
    "dev": "vite",
    "dev:prd": "vite --mode production",
    "dev:tst": "vite --mode testing",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "preview": "vite preview",
    "release": "sa release",
    "typecheck": "vue-tsc --noEmit --skipLibCheck",
    "update-pkg": "sa update-pkg"
  },
  "dependencies": {
    "@antv/data-set": "0.11.8",
    "@antv/g2": "5.2.10",
    "@antv/g6": "5.0.35",
    "@better-scroll/core": "2.5.1",
    "@iconify/vue": "4.2.0",
    "@sa/axios": "workspace:*",
    "@sa/color": "workspace:*",
    "@sa/hooks": "workspace:*",
    "@sa/materials": "workspace:*",
    "@sa/utils": "workspace:*",
    "@visactor/vchart": "1.13.1",
    "@visactor/vchart-theme": "1.12.2",
    "@visactor/vtable-editors": "1.13.2",
    "@visactor/vtable-gantt": "1.13.2",
    "@visactor/vue-vtable": "1.13.2",
    "@vue-flow/background": "1.3.2",
    "@vue-flow/core": "1.41.6",
    "@vueuse/components": "12.0.0",
    "@vueuse/core": "12.0.0",
    "ag-grid-vue3": "33.0.2",
    "clipboard": "2.0.11",
    "d3": "7.9.0",
    "dayjs": "1.11.13",
    "defu": "6.1.4",
    "dhtmlx-gantt": "9.0.3",
    "echarts": "5.5.1",
    "html-to-image": "1.11.11",
    "intro.js": "7.2.0",
    "json-editor-vue": "0.17.3",
    "json5": "2.2.3",
    "jspdf": "2.5.2",
    "naive-ui": "2.40.3",
    "nprogress": "0.2.0",
    "pagedjs": "0.4.3",
    "pinia": "2.3.0",
    "signature_pad": "5.0.4",
    "swiper": "11.1.15",
    "tailwind-merge": "2.5.5",
    "typeit": "8.8.7",
    "vue": "3.5.13",
    "vue-draggable-plus": "0.6.0",
    "vue-i18n": "10.0.5",
    "vue-json-pretty": "2.4.0",
    "vue-pdf-embed": "2.1.1",
    "vue-router": "4.5.0",
    "vue3-google-map": "0.21.0",
    "wavesurfer.js": "7.8.11",
    "xgplayer": "3.0.20",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.12.0",
    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@iconify/json": "2.2.285",
    "@sa/scripts": "workspace:*",
    "@types/d3": "7.4.3",
    "@types/dagre": "0.7.52",
    "@types/html-minifier-terser": "7.0.2",
    "@types/intro.js": "5.1.5",
    "@types/node": "22.10.2",
    "@types/nprogress": "0.2.3",
    "@unocss/eslint-config": "0.65.1",
    "@unocss/preset-icons": "0.65.1",
    "@unocss/preset-uno": "0.65.1",
    "@unocss/transformer-directives": "0.65.1",
    "@unocss/transformer-variant-group": "0.65.1",
    "@unocss/vite": "0.65.1",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "dagre": "0.8.5",
    "eslint": "9.17.0",
    "eslint-plugin-format": "0.1.3",
    "html-minifier-terser": "7.2.0",
    "lint-staged": "15.2.11",
    "sass": "1.83.0",
    "simple-git-hooks": "2.11.1",
    "tsx": "4.19.2",
    "typescript": "5.6.2",
    "unplugin-auto-import": "0.19.0",
    "unplugin-icons": "0.22.0",
    "unplugin-vue-components": "0.28.0",
    "unplugin-vue-router": "0.10.9",
    "vite": "5.4.11",
    "vite-plugin-svg-icons": "2.0.1",
    "vite-plugin-vue-devtools": "7.6.8",
    "vite-plugin-vue-layouts": "0.11.0",
    "vue-tsc": "2.1.10"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "eslint": "9.17.0"
      }
    }
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm commit",
    "pre-commit": "pnpm typecheck && pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "volta": {
    "node": "22.11.0",
    "pnpm": "9.15.0"
  }
}
