    "dagre": "0.8.5",
    "eslint": "9.39.2",
    "eslint-plugin-format": "1.1.0",
    "html-minifier-terser": "7.2.0",
    "lint-staged": "16.2.7",
    "sass": "1.97.1",
    "simple-git-hooks": "2.13.1",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
